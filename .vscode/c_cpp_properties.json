{
    "configurations": [
        {
            "name": "ROS2",
            "browse": {
                "databaseFilename": "${default}",
                "limitSymbolsToIncludedHeaders": true   //智能提示只搜索包含的头文件，速度快
            },
            "includePath": [
                "/usr/local/include/ceres/**",
                "/opt/ros/foxy/include/**",
                "/usr/local/include/**",
                "/usr/include/**",
                "${workspaceFolder}/src/**"
            ],
            "intelliSenseMode": "${default}",
            "compilerPath": "/usr/bin/gcc",
            "cStandard": "gnu17",
            "cppStandard": "c++17",
            "configurationProvider": "ms-vscode.cmake-tools"
        }
    ],
    "version": 4
}
